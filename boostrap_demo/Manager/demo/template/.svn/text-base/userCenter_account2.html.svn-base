<meta charset="UTF-8">
<form action="#" method="post" id="theForm" class="form-column">
	<div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="btsId">ID:</label>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                   	<input type="text" ng-model="user.btsId" class="form-control" placeholder="" name="btsId" id="btsId" />
                </div>
            </div>
        </div>
    </div>
    
    <div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="btsName">网元名称:</label>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                    <input type="text" ng-model="user.btsName" class="form-control" id="btsName" />
                </div>
            </div>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="btsType">网元类型:</label>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                   <input type="text" ng-model="user.btsType" class="form-control" id="btsType" />
                </div>
            </div>
        </div>
    </div>
</form>