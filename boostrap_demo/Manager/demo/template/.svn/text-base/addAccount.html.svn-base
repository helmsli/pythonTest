<meta charset="UTF-8">
<div class="modal-dialog" ng-controller="addUser">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title" id="myModalLabel">新建员工</h4>
        </div>
        <div class="modal-body">       	
        	<form action="#" method="post" id="theForm" class="form-custom">
				<div class="form-group">
					<label class="form-label">账号名：</label>
					<input type="text" class="form-control" name="userName" id="userName" data-error="UserName error" data-tip="Please enter the user name" />
				</div>
				<!-- / User Name -->
				<div class="form-group">
					<label class="form-label">密码：</label>
					<input type="text" class="form-control" id="pwd" data-error="password error" data-tip="Please enter your password" />
					<p></p>
					<div></div>
				</div>
				<!-- / Password -->
				<div class="form-group">
					<label class="form-label">确认密码</label>
					<input type="text" class="form-control" id="conPwd" data-error="password error" data-tip="Please enter your password" />
				</div>
				<!-- / Password -->
				<div class="form-group">
					<label class="form-label">员工姓名</label>
					<input type="text" class="form-control" name="userName" id="userName" data-error="UserName error" data-tip="Please enter the user name" />
				</div>
				<!-- / User Name -->
			</form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" data-dismiss="modal">关闭</button>
            <button type="button" class="btn btn-success" ng-click="editUser()">提交更改</button>
        </div>
    </div>
</div>
<script src="../js/formValidata/getFormData.js" type="text/javascript"></script>
