<meta charset="UTF-8">
<form action="#" method="post" id="theForm" class="form-column">
	<div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="email_address_2">Name:</label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                    <input type="text" class="form-control" placeholder="user name" name="userName" id="userName" data-error="UserName error" data-tip="Please enter the user name" />
                </div>
            </div>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="email_address_2">True name:</label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                    <input type="text" class="form-control" placeholder="true name" name="userName" id="pwd" data-error="UserName error" data-tip="Please enter the user name" />
                </div>
            </div>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="email_address_2">Password:</label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                    <input type="text" class="form-control" placeholder="password" name="userName" id="email" data-error="UserName error" data-tip="Please enter the user name" />
                </div>
            </div>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="email_address_2">Confirm password:</label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                    <input type="text" class="form-control" placeholder="confirm password" name="userName" id="myNum" data-error="UserName error" data-tip="Please enter the user name" />
                </div>
            </div>
        </div>
    </div>
    <div class="row clearfix">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 form-control-label">
            <label for="email_address_2">Sex:</label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <div class="form-group">
                <div class="form-line">
                	<div class="radio">
                		<div class="radio-inline">
                			<input type="radio" id="radio_1" class="with-gap radio-col-blue" name="hobby" value="sing" data-error="未勾选" />
                        	<label for="radio_1">Man</label>
                		</div>
                		<div class="radio-inline">
                			<input type="radio" id="radio_2" class="with-gap radio-col-blue" name="hobby" value="dance" data-error="未勾选" />
                        	<label for="radio_2">Woman</label>
                		</div>
                    </div>
                </div>
                <div class="input-tips">
					<label class="error" id="checkboxError">请勾选</label>
				</div>
            </div>
        </div>
    </div>
    
	
</form>
<script src="../../../js/formValidata/getFormData.js" type="text/javascript"></script>