@font-face
{
	font-family:'Open Sans';
	font-style:normal;
	font-weight:400;
	src:local('Open Sans'),url(../../fonts/open-sans.eot?#iefix) format('embedded-opentype'),url(../../fonts/open-sans.eot),url(../../fonts/open-sans.woff) format('woff'),url(../../fonts/open-sans.ttf) format('truetype');
}
html,body{
	margin:0;
	padding:0;
}
body{
	font-family:Open Sans, Arial, Helvetica, sans-serif;
	/*-moz-transition: all 0.5s;
	-o-transition: all 0.5s;
	-webkit-transition: all 0.5s;
	transition: all 0.5s;*/
}
a {
	color:#333;
    text-decoration: none;
}

a:link, a:visited ,a:hover, a:active{
    text-decoration: none;
}  

a:focus, input, textarea {
	outline:none;
	-moz-outline-style: none;
}  

ul,ol,li{
	list-style: none;
}

.fl{
	float: left;
}

.fr{
	float: right;
}

/*icon*/
.icon{
	display:inline-block;
	width:22px;
	height:22px;
	vertical-align:0;
}
.icon img{
	width:18px;
	height:18px;
}
/*
 * wrapper
 */
#wrapper {
    width: 100%;
}
@media(max-width:768px){
	#page-wrapper {
	    padding: 85px 15px 1px 15px;
	}	
}
@media(min-width:769px) {
    #page-wrapper {
        margin-left: 250px;
	    padding: 100px 30px 10px 30px;
	    min-height: 568px;
	    border-left: 1px solid #e0e0e0;
    }
}
.header{
	position: fixed;
	top:0;
	right:0;
	left:0;
	z-index:1030;
	height:70px;
}
.menus{
	width:250px;
}
.navbar-fixed-bottom .navbar-collapse, .navbar-fixed-top .navbar-collapse{
	max-height:100%!important;
}
.pa-lt-no{
	padding-left:0px;
}
.pa-rt-no{
	padding-right:0px;
}
/*
 * navbar-top
 */
.navbar {
	margin-bottom: 0;
	min-height: 70px;
    box-shadow: 0 1px 5px rgba(0, 0, 0, 0.3);
}
@media(max-width:768px){
	.navbar-header{
		position: absolute;
		top: 0;
		left: 50%;
		-webkit-transform:translate(-50%,0%);
		transform:translate(-50%,0%);
	}
}
@media(min-width:768px){
	.navbar-header {
	    float: left;
	}	
}

.navbar-inverse {
	border: 0 none;
}
.navbar-inverse .navbar-brand{
	display: inline-block;
	padding: 0 15px;
	height: 70px;
	line-height: 70px;
	font-size: 22px;
}

@media(min-width:769px) {
   .navbar-inverse .navbar-brand {
        padding: 8px 15px;
		width: 162px;
		height: 70px;
		text-indent: -9999px;
    }
}
@media(max-width:768px) {
	.navbar-toggle {
	    display: block;
	}
}
@media(min-width:769px) {
	.navbar-toggle {
	    display: none;
	}
}

.navbar-inverse .navbar-nav {
	display: none!important;
}

@media(min-width:767px) {
   .navbar-inverse .navbar-nav {
        display: block!important;
    }
}
@media(min-width:768px) and (max-width:990px) {
   .navbar-inverse .navbar-nav {
        display: none!important;
    }
}

.navbar-inverse .navbar-nav li{
	height:70px;
}

.navbar-inverse .navbar-nav li a{
	display:block;
	height:100%;
	line-height:48px;
}
.navbar-inverse .navbar-left{
	margin-left: 88px;
}

.navbar-inverse .navbar-toggle {
	margin: 28px 0 0 15px;
	padding: 0;
	border: 0;
	border-radius: 0px;
	float: left;
}

.navbar-inverse .navbar-toggle:focus,
.navbar-inverse .navbar-toggle:hover {
	background: none;
}

.navbar-right.navbar-top-links {
	float: right;
}
.navbar-top-links {
    margin: 0;
}
@media(min-width:769px) and (max-width:992px){
   .navbar-top-links {
        display: none!important;
    }
}

.navbar-top-links li {
    display: inline-block;
}

.navbar-top-links li a {
    padding:21px 15px;
    min-height: 70px;
}
@media(min-width:769px){
	.navbar-top-links li:last-child {
	    margin-right: 15px;
	}
	.navbar-top-links li a {
	    padding:21px 15px;
	    min-height: 70px;
	}	
}

.navbar-top-links .user-profile img{
	display: none;
	width: 28px;
	height: 28px;
	border-radius: 50%;
	border: solid 2px #fff;
	vertical-align: -10px;
}

.navbar-top-links .user-profile .fa.fa-user{
	line-height: 26px;
	font-size: 18px;
}
@media(min-width:769px){
	.navbar-top-links .user-profile img {
		display: inline-block;
	}
	.navbar-top-links .user-profile .fa.fa-user{
		display: none;
	}
}

.navbar-top-links .dropdown,
.navbar-top-links .dropdown-menu {
	font-size: 13px;	
}

.navbar-top-links .dropdown em{
	display: none;
	font-style:normal;
	font-size:13px;
}
.navbar-top-links .dropdown .caret {
	display: none;
}

@media(min-width:769px){
	.navbar-top-links .dropdown em{
		display: inline-block;
		font-style:normal;
		font-size:13px;
	}
	.navbar-top-links .dropdown .caret {
		display: inline-block;
	}	
}

.navbar-top-links .dropdown-toggle.skin-fa {
	line-height: 25px;
	font-size: 18px;
}

@media(min-width:769px){
	.navbar-top-links .dropdown-toggle.skin-fa {
		line-height: 23px;
	}
}

.navbar-top-links .dropdown-menu li {
    display: block;
}

.navbar-top-links .dropdown-menu li:last-child {
    margin-right: 0;
}

.navbar-top-links .dropdown-menu li a {
    padding: 3px 20px;
    min-height: 0;
}

.navbar-top-links .dropdown-menu li a div {
    white-space: normal;
}

.navbar-top-links .dropdown-messages,
.navbar-top-links .dropdown-tasks,
.navbar-top-links .dropdown-alerts {
    width: 310px;
    min-width: 0;
}

.navbar-top-links .dropdown-messages {
    margin-left: 5px;
}

.navbar-top-links .dropdown-tasks {
    margin-left: -59px;
}

.navbar-top-links .dropdown-alerts {
    margin-left: -123px;
}

.navbar-top-links .dropdown-user {
    right: 0;
    left: auto;
}

.navbar-top-links .dropdown-messages,
.navbar-top-links .dropdown-tasks,
.navbar-top-links .dropdown-alerts {
    margin-left: auto;
}


/*skin*/
#skin span{
	display:block;
	padding: 6px 15px;
	cursor:pointer;
}
#skin .fa{
	font-size: 16px;
}

/*
 * navbar-left
 */

